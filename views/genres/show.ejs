<h2><%= genre.name %></h2>
<br>
<a href="/artists/<%= artist.id %>/edit"> Edit </a>
<%- include('../partials/deleteForm', { url: `/artists/${artist.id}`}) %>

<% if(setsByArtist.length > 0 ) { %>
    <h2> Sets By Artist </h2>

    <% setsByArtist.forEach(set => { %>
        <div><%= set.title %> </div>
        <a target="_blank" rel="noopener noreferrer" href=" <%= set.link %> "> Link </a>
    <% }) %>
<% } %>