<h2 class="page-header"><%= artist.name %></h2>
<img height="240" width="240" src="<%= artist.headshotPath %>">
<br>
<h3> Edit Artist </h3>
<form action="/artists/<%= artist.id %>?_method=PUT" method="POST">
    <%- include('_form_fields') %>
    <a class="btn btn-primary" href="/artists/<%= artist.id %> "> Cancel </a>
    <button class="btn btn-primary" type="submit"> Update </button>
</form>